{
  "expo": {
    "name": "BabyCare",
    "slug": "babycare",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/babycare_logo.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["expo-router"],
    "orientation": "portrait",
    "icon": "./assets/babycare_logo_rb.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/babycare_logo_rb.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/babycare_logo_rb.png",
        "backgroundColor": "#ffffff"
      },
      "newArchEnable": true,
      "package": "com.babycare.pbl5app"
    },
    "extra": {
      "eas": {
        "projectId": "68033c70-f1d9-4cab-a6d8-7b428a21ff94"
      }
    }
  }
}
